{
  "name": "Picturebot",
  "version": "2.0.0-alpha.1",
  "description": "Angular 10 with Electron (Typescript + SASS + Hot Reload)",
  "homepage": "https://github.com/Tomekske/Picturebot",
  "author": {
    "name": "Tomek Joostens",
    "email": "joostenstomek@gmail.com"
  },
  "keywords": [
    "angular",
    "angular 10",
    "electron",
    "typescript",
    "eslint",
    "spectron",
    "sass"
  ],
  "main": "main.js",
  "private": true,
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "ng": "ng",
    "start": "npm-run-all -p electron:serve ng:serve",
    "build": "npm run electron:serve-tsc && ng build --base-href ./",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "ng:serve": "ng serve",
    "ng:serve:web": "ng serve -c web -o",
    "electron:serve-tsc": "tsc -p tsconfig-serve.json",
    "electron:serve": "wait-on tcp:4200 && npm run electron:serve-tsc && npx electron . --serve",
    "electron:local": "npm run build:prod && npx electron .",
    "electron:build": "npm run build:prod && electron-builder build",
    "test": "mocha",
    "test:watch": "ng test",
    "version": "conventional-changelog -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "lint": "ng lint",
    "msi": "electron-builder",
    "prerelease": "npm run build -- --prod",
    "git": "npm run build:prod && electron-builder",
    "release": "npm run build:prod"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "9.2.0",
    "@angular-devkit/build-angular": "0.1000.2",
    "@angular-eslint/builder": "0.0.1-alpha.32",
    "@angular-eslint/eslint-plugin": "0.0.1-alpha.32",
    "@angular-eslint/eslint-plugin-template": "0.0.1-alpha.32",
    "@angular-eslint/template-parser": "0.0.1-alpha.32",
    "@angular/cli": "10.0.2",
    "@angular/common": "10.0.3",
    "@angular/compiler": "10.0.3",
    "@angular/compiler-cli": "10.0.3",
    "@angular/core": "10.0.3",
    "@angular/forms": "10.0.3",
    "@angular/language-service": "10.0.3",
    "@angular/platform-browser": "10.0.3",
    "@angular/platform-browser-dynamic": "10.0.3",
    "@angular/router": "10.0.3",
    "@cucumber/cucumber": "7.0.0-rc.0",
    "@ngx-translate/core": "13.0.0",
    "@ngx-translate/http-loader": "6.0.0",
    "@types/jasmine": "3.5.11",
    "@types/jasminewd2": "2.0.8",
    "@types/mocha": "7.0.2",
    "@types/node": "12.12.6",
    "@typescript-eslint/eslint-plugin": "3.6.0",
    "@typescript-eslint/eslint-plugin-tslint": "3.6.0",
    "@typescript-eslint/parser": "3.6.0",
    "chai": "4.2.0",
    "conventional-changelog-cli": "2.0.34",
    "core-js": "3.6.5",
    "cross-env": "7.0.2",
    "electron": "9.1.1",
    "electron-builder": "22.7.0",
    "electron-packager": "15.0.0",
    "electron-reload": "1.5.0",
    "electron-wix-msi": "3.0.3",
    "eslint": "7.4.0",
    "eslint-plugin-import": "2.22.0",
    "jasmine-core": "3.5.0",
    "jasmine-spec-reporter": "5.0.2",
    "karma": "5.1.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-electron": "6.3.0",
    "karma-jasmine": "3.3.1",
    "karma-jasmine-html-reporter": "1.5.4",
    "mocha": "7.2.0",
    "npm-run-all": "4.1.5",
    "rxjs": "6.6.0",
    "spectron": "11.1.0",
    "ts-node": "8.10.2",
    "tslib": "2.0.0",
    "typescript": "3.9.6",
    "wait-on": "5.0.1",
    "webdriver-manager": "^12.1.7",
    "zone.js": "0.10.3"
  },
  "engines": {
    "node": ">=10.13.0"
  },
  "dependencies": {
    "@angular/animations": "^10.0.0 || ^11.0.0-0",
    "@angular/cdk": "10.2.5",
    "@angular/flex-layout": "10.0.0-beta.32",
    "@angular/forms": "^10.0.0 || ^11.0.0-0",
    "@angular/localize": "10.1.6",
    "@angular/material": "10.2.5",
    "@angular/material-moment-adapter": "11.0.0",
    "@crystalui/angular-lightbox": "1.1.8",
    "@fortawesome/fontawesome-free": "5.14.0",
    "@ng-bootstrap/ng-bootstrap": "7.0.0",
    "@ng-bootstrap/schematics": "2.0.0-alpha.1",
    "@popperjs/core": "2.5.3",
    "@syncfusion/ej2-angular-lists": "18.3.35",
    "@types/better-sqlite3": "5.4.0",
    "better-sqlite3": "7.1.0",
    "body-parser": "1.19.0",
    "bootstrap": "4.5.3",
    "bootstrap-material-design": "4.1.1",
    "electron-chromedriver": "^11.0.0",
    "electron-updater": "4.3.5",
    "hasha": "5.2.1",
    "jquery": "3.5.1",
    "log4js": "6.3.0",
    "material-icons": "0.3.1",
    "moment": "2.29.1",
    "ng-material-multilevel-menu": "4.12.2",
    "ngx-dropzone": "2.2.2",
    "popper.js": "1.16.1"
  },
  "build": {
    "appId": "Picturebot",
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "src/assets/icons/favicon.ico"
    },
    "nsis": {
      "installerIcon": "src/assets/icons/favicon.ico",
      "uninstallerIcon": "src/assets/icons/favicon.ico",
      "uninstallDisplayName": "Picturebot",
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "directories": {
      "output": "release/"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "Tomekske",
        "repo": "Picturebot"
      }
    ]
  }
}
